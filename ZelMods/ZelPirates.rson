"",
"while (days >= reload) {",
"  pirate = BuyPirate(PlanetPirateClan(), 100 + 40 * level);",
"  random = Rnd(Maloc, Gaal);",
"  ShipPilotRace(pirate, random);",
"  ShipOwner(pirate, 7);",
"  ItemOwner(ShipItems(pirate, 0), random);",
"  hullSize = 250 + (15 + 3 * level) * GalaxyTechLevel();",
"  hullSize = Rnd(0.7 * hullSize, 1.3 * hullSize);",
"  for (i = 0; i < 10; i = i + 1)",
"    ShipRefit(pirate, hullSize);",
"  Chameleon(pirate);",
"  ShipImproveItems(pirate, level);",
"  days = days - reload;",
"}"          
