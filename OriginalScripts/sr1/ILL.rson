        {
          "Type":  "TDialogMsg",
          "Name":  "",
          "Pos.x":  970,
          "Pos.y":  730,
          "Parent":  -1,
          "#":  38,
          "Msg":  "Привез? Подлети поближе.",
          "DMsg.Num":  "5"        
