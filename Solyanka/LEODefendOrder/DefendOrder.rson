        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  650,
          "Pos.y":  280,
          "Parent":  -1,
          "#":  91,
          "Total.Lines":  357,
          "Code.Type":  "Init",
          "Code":          
          [
              "function OnUseCodeDefendOrder()",
              "{",
              "    // ForArtInfo();",
              "    // SFT('Активация артефакта');",
              "    vNearCivilPlanet = NearCivilPlanet(Player());",
              "    vDistNearCivilPlanet = Dist(Player(), vNearCivilPlanet);",
              "    vGovernmentType = PlanetGov(NearCivilPlanet(Player()));",
              "    vFleetFinance = CalculateFinancePlanet(vGovernmentType);",
              "    vWarShipsInSystem = WarShipsNumber();",
              "    vStarOwner = StarOwner(ShipStar(Player()));",
              "",
              "    if (StarBattle(ShipStar(Player())) == 0)",
              "    {",
              "        MessageBox(CT('UselessItems.DefendOrder.ItemMessages.notAttacked'));",
              "    }",
              "    if (StarBattle(ShipStar(Player())) == 1)",
              "    {",
              "        if (vWarReserves < activatingReservesReq)",
              "        {",
              "            MessageBox(CT('UselessItems.DefendOrder.ItemMessages.lowReserves'));",
              "        }",
              "        if (vWarReserves >= activatingReservesReq)",
              "        {",
              "            if (vWarShipsInSystem > 5)",
              "            {",
              "                MessageBox(CT('UselessItems.DefendOrder.ItemMessages.aLotOfWarShips'));",
              "            }",
              "            if (vWarShipsInSystem <= 5)",
              "            {",
              "                if (ShipRadarRange(Player()) < vDistNearCivilPlanet)",
              "                {",
              "                    // SFT('Радар не достаёт до планеты');",
              "                    MessageBox(CT('UselessItems.DefendOrder.ItemMessages.outOfRange'));",
              "                }",
              "                if (ShipRadarRange(Player()) >= vDistNearCivilPlanet)",
              "                {",
              "                    // SFT('Арт: Радар достигает планеты');",
              "                    if (vStarOwner == 2)",
              "                    {",
              "                        Sound('Sound.DefendOrderSignal');",
              "                        Ether(1, '', PirateAnswer());",
              "                    }",
              "                    if (vStarOwner == 1)",
              "                    {",
              "                        Sound('Sound.DefendOrderSignal');",
              "                        Ether(1, '', DomAnswer());",
              "                    }",
              "                    if (vStarOwner == 0)",
              "                    {",
              "                        if (ShipOnSidePirateClan(Player()) == 0)",
              "                        {",
              "                            // SFT('Арт: Игрок не в пиратском клане');",
              "                            Sound('Sound.DefendOrderSignal');",
              "                            CreateFleet(ShipRank(Player()), vNearCivilPlanet, vFleetFinance);",
              "                            if (vFirstConsumedOrder == 0)",
              "                                vFirstConsumedOrder = 1;",
              "                        }",
              "                        if (ShipOnSidePirateClan(Player()) == 1)",
              "                        {",
              "                            Ether(1, '', CoalAnswerPlPirate());",
              "                        }",
              "                    }",
              "                }",
              "            }",
              "        }",
              "    }",
              "}",
              "",
              "//Расчет финансирования планеты от гос.строя",
              "//Если радар игрока достигает ближ. нас. планету",
              "//Если игрок не на стороне пиратов",
              "//Запросить строительство кораблей в зависимости от звания игрока;",
              "function CreateFleet(playerRank, planet, fleetFinance)",
              "{",
              "    // SFT('Создаётся Флот');",
              "    if (vFleetLvl == 1)",
              "    {",
              "        CreateFleetLvl1(playerRank, planet, fleetFinance);",
              "    }",
              "    if (vFleetLvl == 2)",
              "    {",
              "        CreateFleetLvl2(playerRank, planet, fleetFinance);",
              "    }",
              "}",
              "",
              "function CreateFleetLvl1(playerRank, planet, fleetFinance)",
              "{",
              "    vWarReserves = vWarReserves - 300;",
              "    UselessItemData(DefOrd, 1, vWarReserves);",
              "",
              "    if (playerRank == 3)",
              "    {",
              "        // SFT('Флот ранга Капитан');",
              "        vWarriorShip1 = BuyWarrior(planet, fleetFinance);",
              "        vWarriorShip1 = BuyWarrior(planet, fleetFinance);",
              "        FleetReport(2);",
              "    }",
              "    if (playerRank == 4)",
              "    {",
              "        // SFT('Флот ранга Истребитель');",
              "        vWarriorShip1 = BuyWarrior(planet, fleetFinance);",
              "        vWarriorShip1 = BuyWarrior(planet, fleetFinance);",
              "        vWarriorShip1 = BuyWarrior(planet, fleetFinance);",
              "        FleetReport(3);",
              "    }",
              "    if (playerRank == 5)",
              "    {",
              "        // SFT('Флот ранга Ас');",
              "        vWarriorShip1 = BuyWarrior(planet, fleetFinance);",
              "        vWarriorShip1 = BuyWarrior(planet, fleetFinance);",
              "        vWarriorShip1 = BuyWarrior(planet, fleetFinance);",
              "        vWarriorShip1 = BuyWarrior(planet, fleetFinance);",
              "        FleetReport(4);",
              "    }",
              "    if (playerRank == 6)",
              "    {",
              "        // SFT('Флот ранга Командор');",
              "        vWarriorShip1 = BuyWarrior(planet, fleetFinance);",
              "        vWarriorShip1 = BuyWarrior(planet, fleetFinance);",
              "        vWarriorShip1 = BuyWarrior(planet, fleetFinance);",
              "        vWarriorShip2 = BuyBigWarrior(planet, fleetFinance);",
              "        FleetReport(4);",
              "    }",
              "    if (playerRank == 7)",
              "    {",
              "        // SFT('Флот ранга Адмирал');",
              "        vWarriorShip1 = BuyWarrior(planet, fleetFinance);",
              "        vWarriorShip1 = BuyWarrior(planet, fleetFinance);",
              "        vWarriorShip1 = BuyWarrior(planet, fleetFinance);",
              "        vWarriorShip1 = BuyWarrior(planet, fleetFinance);",
              "        vWarriorShip2 = BuyBigWarrior(planet, fleetFinance);",
              "        vWarriorShip2 = BuyBigWarrior(planet, fleetFinance);",
              "        FleetReport(6);",
              "    }",
              "}",
              "",
              "function CreateFleetLvl2(playerRank, planet, fleetFinance)",
              "{",
              "    vSmallShips = 0;",
              "    vFlagmanShips = 0;",
              "    vWarReserves = vWarReserves - 300;",
              "    UselessItemData(DefOrd, 1, vWarReserves);",
              "",
              "    CountBuildFleet(playerRank);",
              "    BuildSmallFleetLvl2(planet, fleetFinance);",
              "    BuildFlagmanFleetLvl2(planet, fleetFinance);",
              "    FleetReport(vSmallShips + vFlagmanShips);",
              "}",
              "//Создать флот в зависимости от звания игрока и типа строя планеты",
              "//обнулить военные резервы у артефакта",
              "",
              "function CountBuildFleet(playerRank)",
              "{",
              "    if (playerRank == 3)",
              "    {",
              "        vSmallShips = 2;",
              "    }",
              "    if (playerRank == 4)",
              "    {",
              "        vSmallShips = 3;",
              "    }",
              "    if (playerRank == 5)",
              "    {",
              "        vSmallShips = 4;",
              "    }",
              "    if (playerRank == 6)",
              "    {",
              "        vSmallShips = 4;",
              "        vFlagmanShips = 1;",
              "    }",
              "    if (playerRank == 7)",
              "    {",
              "        vSmallShips = 6;",
              "        vFlagmanShips = 2;",
              "    }",
              "}",
              "//Подсчет количества кораблей которых нужно построить",
              "//заисит от ранга игрока",
              "",
              "function BuildSmallFleetLvl2(planet, fleetFinance)",
              "{",
              "    for (i = 0; i < vSmallShips; i = i + 1)",
              "    {",
              "        vWarriorShip1 = BuyWarrior(planet, fleetFinance);",
              "        // SFT('ship name: '+ Name(vWarriorShip1));",
              "        vShipNumberItems = ShipItems(vWarriorShip1);",
              "        for (a = 0; a < vShipNumberItems; a = a + 1)",
              "        {",
              "            if (ItemLevel(ShipItems(vWarriorShip1, a)) < GalaxyTechLevel())",
              "            {",
              "                ItemLevel(ShipItems(vWarriorShip1, a), GalaxyTechLevel());",
              "            }",
              "        }",
              "        if (HullHP(vWarriorShip1) < vShipHullFactorSmall * GalaxyTechLevel())",
              "        {",
              "            ItemSize(ShipItems(vWarriorShip1, 0), vShipHullFactorSmall * GalaxyTechLevel());",
              "            HullHP(vWarriorShip1, vShipHullFactorSmall * GalaxyTechLevel());",
              "        }",
              "        if (ShipEqInSlot(vWarriorShip1, t_DefGenerator) == 0)",
              "        {",
              "            vFleetEquipment = CreateEquipment(t_DefGenerator, Rnd(20, 60), GalaxyTechLevel(), GetOwner(vWarriorShip1));",
              "            AddItemToShip(vWarriorShip1, vFleetEquipment);",
              "        }",
              "        if (ShipEqInSlot(vWarriorShip1, t_RepairRobot) == 0)",
              "        {",
              "            vFleetEquipment = CreateEquipment(t_RepairRobot, Rnd(20, 60), GalaxyTechLevel(), GetOwner(vWarriorShip1));",
              "            AddItemToShip(vWarriorShip1, vFleetEquipment);",
              "        }",
              "        if (Rnd(0, 1) == 1)",
              "            ShipImproveItems(vWarriorShip1, 1);",
              "        ArrangeItems(vWarriorShip1);",
              "    }",
              "}",
              "//Малые корабли",
              "//Подсчет оборудования на корабле, обновления оборудования на текущий тех. уровень",
              "//увеличить HullHP если имало",
              "//Добавление если нет, ГЗП",
              "//Добавление если нет, Ремонтного дроида",
              "//Улучшение случайных предметов на борту",
              "",
              "function BuildFlagmanFleetLvl2(planet, fleetFinance)",
              "{",
              "    for (i = 0; i < vFlagmanShips; i = i + 1)",
              "    { // big ship",
              "        vWarriorShip2 = BuyBigWarrior(planet, fleetFinance);",
              "        // SFT('ship name: '+ Name(vWarriorShip2));",
              "        vShipNumberItems = ShipItems(vWarriorShip2);",
              "        for (a = 0; a < vShipNumberItems; a = a + 1)",
              "        {",
              "            if (ItemLevel(ShipItems(vWarriorShip2, a)) < GalaxyTechLevel())",
              "            {",
              "                ItemLevel(ShipItems(vWarriorShip2, a), GalaxyTechLevel());",
              "            }",
              "        }",
              "        if (HullHP(vWarriorShip2) < vShipHullFactorFlagman * GalaxyTechLevel())",
              "        {",
              "            ItemSize(ShipItems(vWarriorShip2, 0), vShipHullFactorFlagman * GalaxyTechLevel());",
              "            HullHP(vWarriorShip2, vShipHullFactorFlagman * GalaxyTechLevel());",
              "        }",
              "        if (ShipEqInSlot(vWarriorShip2, t_DefGenerator) == 0)",
              "        {",
              "            vFleetEquipment = CreateEquipment(t_DefGenerator, Rnd(20, 60), GalaxyTechLevel(), GetOwner(vWarriorShip2));",
              "            AddItemToShip(vWarriorShip2, vFleetEquipment);",
              "        }",
              "        if (ShipEqInSlot(vWarriorShip2, t_RepairRobot) == 0)",
              "        {",
              "            vFleetEquipment = CreateEquipment(t_RepairRobot, Rnd(20, 60), GalaxyTechLevel(), GetOwner(vWarriorShip2));",
              "            AddItemToShip(vWarriorShip2, vFleetEquipment);",
              "        }",
              "        ShipImproveItems(vWarriorShip2, 3);",
              "        ArrangeItems(vWarriorShip2);",
              "    }",
              "}",
              "//Флагманы",
              "//Подсчет оборудования на корабле, обновления оборудования на текущий тех. уровень",
              "//увеличить HullHP если имало",
              "//Добавление если нет, ГЗП",
              "//Добавление если нет, Ремонтного дроида",
              "//Улучшение случайных предметов на борту;",
              "function ForArtInfo()",
              "{",
              "    // SFT('Игрок в системе '+ ShipStar(Player()) );",
              "    SFT('Бой в системе: ' + StarBattle(ShipStar(Player())));",
              "    SFT('Ближайшая заселенная планета к игроку: ' + PlanetName(NearCivilPlanet(Player())));",
              "    // SFT('Строй на ближ. планете: '+ PlanetGov(NearCivilPlanet(Player())) );",
              "    SFT('Радиус радара игрока: ' + ShipRadarRange(Player()));",
              "    SFT('Расстояние от игрока до планеты: ' + Dist(Player(), NearCivilPlanet(Player())));",
              "    // SFT('Игрок состоит в пиратском клане: '+ ShipOnSidePirateClan(Player()) );",
              "    // SFT('Отношение к рейнджеру планетой: '+ RelationToRanger( NearCivilPlanet(Player()), Player() ));",
              "    // SFT('Очки коалиционного рейтинга: '+ ShipRankPoints(Player()) );",
              "    SFT('Звание игрока: ' + ShipRank(Player()));",
              "    SFT('Уровень резервов в предмете: ' + vWarReserves);",
              "    // ShipRankPoints(Player(), ShipRankPoints(Player())+100 );",
              "    //Функция для сбора информации",
              "}",
              "",
              "function FleetReport(shipsNumber)",
              "{",
              "    Ether(0, '',",
              "          CT('UselessItems.DefendOrder.ItemMessages.repMobilize1') +",
              "              Name(vNearCivilPlanet) +",
              "              CT('UselessItems.DefendOrder.ItemMessages.repMobilize2') +",
              "              shipsNumber +",
              "              CT('UselessItems.DefendOrder.ItemMessages.repMobilize3'));",
              "}",
              "",
              "function CalculateFinancePlanet(plType)",
              "{",
              "    if (plType == 0)",
              "    {",
              "        result = 50;",
              "    }",
              "    if (plType == 1)",
              "    {",
              "        result = 140;",
              "    }",
              "    if (plType == 2)",
              "    {",
              "        result = 100;",
              "    }",
              "    if (plType == 3)",
              "    {",
              "        result = 120;",
              "    }",
              "    if (plType == 4)",
              "    {",
              "        result = 70;",
              "    }",
              "}",
              "function DomAnswer()",
              "{",
              "    if (HaveProgramm('progIntercom') == 0)",
              "    {",
              "        result = vDomAnswer;",
              "    }",
              "    if (HaveProgramm('progIntercom') == 1)",
              "    {",
              "        result = CT('UselessItems.DefendOrder.ItemMessages.domAnswer1');",
              "    }",
              "}",
              "function WarShipsNumber()",
              "{",
              "    vCount = 0;",
              "    vWarShipsInSystem = 0;",
              "    vStarSystem = ShipStar(Player());",
              "    vStarShips = StarShips(ShipStar(Player()));",
              "",
              "    while (vCount < vStarShips)",
              "    {",
              "        // SFT('Тип Корабля в системе: '+ ShipType(StarShips(vStarSystem, vCount)) );",
              "        // SFT('Номер коробля: '+ vCount);",
              "        if (ShipType(StarShips(vStarSystem, vCount)) == 'Warrior')",
              "        {",
              "            vWarShipsInSystem = vWarShipsInSystem + 1;",
              "        }",
              "        vCount = vCount + 1;",
              "    }",
              "    // SFT('Было найдено военных: '+ vWarShipsInSystem );",
              "    result = vWarShipsInSystem;",
              "}",
              "",
              "function PirateAnswer()",
              "{",
              "    result =",
              "        CT('UselessItems.DefendOrder.ItemMessages.PirateMessages.start1') +",
              "        CT('UselessItems.DefendOrder.ItemMessages.PirateMessages.start2') +",
              "        CT('UselessItems.DefendOrder.ItemMessages.PirateMessages.' + Rnd(0, 3));",
              "}",
              "",
              "function CoalAnswerPlPirate()",
              "{",
              "    result =",
              "        CT('UselessItems.DefendOrder.ItemMessages.playerPirate1') +",
              "        PlanetName(vNearCivilPlanet) +",
              "        CT('UselessItems.DefendOrder.ItemMessages.playerPirate2');",
              "}",
              "vPlanetListBlocked = newarray(1);"          
          ]        
        },
        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  680,
          "Pos.y":  280,
          "Parent":  -1,
          "#":  92,
          "Total.Lines":  81,
          "Code.Type":  "Turn",
          "Code":          
          [
              "if (ItemExist(DefOrd) == 1)",
              "{",
              "    // SFT('Сканер. галакт. событий');",
              "    for (i = GalaxyEvents() - 1; i >= 0; i = i - 1)",
              "    {",
              "        if (GalaxyEventDate(i) <= lastCheck)",
              "            break;",
              "        if (GalaxyEventType(i) == 'RuinsCreated')",
              "        {",
              "            // SFT('Создана база');",
              "            if (GalaxyEventData(i, 0) == t_WB)",
              "            {",
              "                // SFT('Создана военная база');",
              "                vNumberWarStations = vNumberWarStations + 1;",
              "                UselessItemData(DefOrd, 3, vNumberWarStations);",
              "            }",
              "        }",
              "        if (GalaxyEventType(i) == 'RuinsDestroyed')",
              "        {",
              "            // SFT('База разрушена');",
              "            if (GalaxyEventData(i, 0) == t_WB)",
              "            {",
              "                // SFT('Уничтожена военная база');",
              "                vNumberWarStations = vNumberWarStations - 1;",
              "                UselessItemData(DefOrd, 3, vNumberWarStations);",
              "            }",
              "        }",
              "    }",
              "",
              "    lastCheck = CurTurn();",
              "",
              "    //Просмотр галк. собитий",
              "    //если создана база, смотреть тип базы, если создана военная",
              "    //увеличить число в vNumberWarStations",
              "    //если база разрушена, смотреть тип базы, если военная",
              "    //уменьшить число в vNumberWarStations ;",
              "    if (vWarReserves < vMaxWarReserves)",
              "    {",
              "        vWarReserves = vWarReserves + vNumberWarStations + 1;",
              "        if (vWarReserves > vMaxWarReserves)",
              "            vWarReserves = vMaxWarReserves;",
              "        UselessItemData(DefOrd, 1, vWarReserves);",
              "",
              "        //Если заряд артефакта ниже максимума",
              "        //Каждый день увеличивать заряд предмета на 1 + количество военных баз",
              "        //Если заряд превышает максимальное число, сделать заряд равным максимальному числу;",
              "    }",
              "}",
              "if (ItemExist(DefOrd) == 2)",
              "{",
              "    SFT('Событие база создана текст: ' + GalaxyEventType('RuinsCreated'));",
