              "            SetName(funct_dwd0, '<color=255,130,0>Гуррит -' + arksNumb + '-</color>');",
              "",
              "            addArkWeapon(funct_dwd0, 'Mauler', Rnd(500, 600), 0);",
              "            addArkWeapon(funct_dwd0, 'Atomizer', Rnd(300, 400), 0);",
              "            addArkWeapon(funct_dwd0, 'frag_gun', Rnd(300, 400), 0);",
              "            addArkWeapon(funct_dwd0, 'laser_gun', Rnd(300, 400), 0);",
              "            addArkWeapon(funct_dwd0, t_Weapon9, Rnd(300, 400), 0);",
              "        }",
              "        //Спавнится Рённак 3",
              "        else if (ShipSubType(funct_dwd0) == 3) {",
              "            funct_int0 = Rnd(1, 2);",
              "            for (funct_j = 0; funct_j < funct_int0; funct_j = funct_j + 1) {",
