              "            SetName(funct_dwd0, '<color=255,130,0>Рённак -' + arksNumb + '-</color>');",
              "",
              "            addArkWeapon(funct_dwd0, 'frag_gun', Rnd(300, 400), 0);",
              "            addArkWeapon(funct_dwd0, 'sphere_gun', Rnd(300, 400), 0);",
              "            addArkWeapon(funct_dwd0, 'Dom_light', Rnd(300, 400), 0);",
              "            addArkWeapon(funct_dwd0, t_Weapon5, Rnd(300, 400), 0);",
