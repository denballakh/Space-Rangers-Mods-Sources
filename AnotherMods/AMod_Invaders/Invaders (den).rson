              "            ShipRepairEq(funct_dwd0);",
              "            ShipType(funct_dwd0, 'AMod_Invaders0');",
              "            SpecialToEquipment(FindBonusByCustomTag('InvadersHull2'), ShipEqInSlot(funct_dwd0, t_Hull));",
