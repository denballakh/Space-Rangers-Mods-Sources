              "        funct_dwd1 = CreateEquipment(type, size, GalaxyTechLevel(), Kling);",
              "    else // type - строка",
              "        funct_dwd1 = CreateCustomWeapon(type, size, GalaxyTechLevel(), Kling);",
              "    EqCustomFaction(funct_dwd1, 'AMod_Invaders');",
              "    if (difCoef >= 3) ItemImprovement(funct_dwd1, 2, 1); // Увеличиваем лучшими спецами первую характеристику (мб урон?)",
              "    SetEquipmentStats(funct_dwd1, dist, 2); // Радиус стрельбы",
              "    AddItemToShip(ship, funct_dwd1);",
              "    ItemIsInUse(funct_dwd1, ship, 1);",
              "    NoDropItem(funct_dwd1, 1);",
              "}",
