              "            SetName(funct_dwd0, '<color=255,130,0>Рондар -' + arksNumb + '-</color>');",
              "",
              "            addArkWeapon(funct_dwd0, 'pulse_gun', Rnd(300, 400), 0);",
              "            addArkWeapon(funct_dwd0, 'Dom_dual_laser', Rnd(300, 400), 0);",
              "            addArkWeapon(funct_dwd0, 'FusionQuasar', Rnd(300, 400), 0);",
              "            addArkWeapon(funct_dwd0, 'Dom_laser', Rnd(300, 400), 0);",
