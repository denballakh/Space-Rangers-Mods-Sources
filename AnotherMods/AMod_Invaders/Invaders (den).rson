          "Parent":  -1,
          "#":  19,
          "Total.Lines":  52,
          "Code.Type":  "Turn",
          "Code":          
          [
              "// Подбор секторов для арксов",
              "for (i = 0; i < GalaxySectors() - 1; i = i + 1) {",
              "    if (int1 == 2) break;",
              "    dwd0 = GalaxySectors(i);",
              "    int0 = 0;",
              "    int1 = 0;",
              "    for (j = 0; j < ConStars(dwd0); j = j + 1) {",
              "        dwd1 = ConStar(dwd0, j);",
              "        if (StarCustomFaction(dwd1) != '') int0 = int0 + 10;",
              "        if (NoComeKlingToStar(dwd1)) int0 = int0 + 10;",
              "        if (ShipStar(Player()) == dwd1) int0 = int0 + 10;",
              "        if (Blazer()) {",
              "            if (ShipStar(Blazer()) == dwd1) int0 = int0 + 10;",
              "        }",
              "        if (Terron()) {",
              "            if (ShipStar(Terron()) == dwd1) int0 = int0 + 10;",
              "        }",
              "        int0 = int0 + 1;",
              "    }",
              "    if (int0 == ConStars(dwd0)) {",
              "        int1 = 0;",
              "        ArrayClear(arr0);",
              "        ArrayAdd(arr0, i);",
              "        for (j = 0; j < GalaxySectors() - 1; j = j + 1) {",
              "            if (int1 == 2) break;",
              "            dwd1 = GalaxySectors(j);",
              "            if (dwd1 == dwd0) continue;",
              "            if (!ConNear(dwd0, dwd1)) continue;",
              "            int0 = 0;",
              "            for (k = 0; k < ConStars(dwd1); k = k + 1) {",
              "                dwd2 = ConStar(dwd1, k);",
              "                if (StarCustomFaction(dwd2) != '') int0 = int0 + 10;",
              "                if (NoComeKlingToStar(dwd2) != 0) int0 = int0 + 10;",
              "                if (ShipStar(Player()) == dwd2) int0 = int0 + 10;",
              "                if (Blazer()) {",
              "                    if (ShipStar(Blazer()) == dwd2) int0 = int0 + 10;",
              "                }",
              "                if (Terron()) {",
              "                    if (ShipStar(Terron()) == dwd2) int0 = int0 + 10;",
              "                }",
              "                if (StarOwner(dwd2) == 1) int0 = int0 + 1;",
              "            }",
              "            if (int0 == ConStars(dwd1)) {",
              "                ArrayAdd(arr0, j);",
              "                int1 = int1 + 1;",
              "            }",
              "        }",
              "    }",
              "}",
              "",
              "// Эзер появления арксов",
              "if (int1 == 2) CustomEther('Galaxy_Alert', 0, 'begin_inv', 'Внимание всем!' + strBR + 'Зафиксированно появление кораблей неизвестной расы. Они атакуют всех без разбора, будь то доминатор или представитель любой разумной органической жизни. До выяснения подробностей рекомендуется воздержаться от полётов в захваченные системы.');"          
          ]        
        },
        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  110,
          "Pos.y":  110,
          "Parent":  -1,
          "#":  21,
          "Total.Lines":  27,
          "Code.Type":  "Init",
          "Code":          
          [
              "difCoef = GalaxyDiffLevels() / 100;",
              "arksNumb = 0;",
              "strBR = substr(CT('Cheat.Ok'), 0, 2);",
              "gunName = newarray(6);",
              "gunName[0] = 'ArksGun';",
              "QState = newarray(4);",
              "arr0 = newarray(1);",
              "arrTC = newarray(11);",
              "research = newarray(11);",
              "arcArray = newarray(4, 4);",
              "spwnDate = newarray(4, 11);",
              "",
              "// Задаём цены из ланга (в обломках арксов) для покупки батлкрузера и лазерганов к нему",
              "needForHull = CT('Items.Weapon.BattlecruiserConfig.BattlecruiserScrapCost');",
              "needForLaserGun = CT('Items.Weapon.BattlecruiserConfig.LaserMissileScrapCost');",
              "for (i = 1; i < 4; i = i + 1) {",
              "    QState[i] = 0;",
              "    arcArray[i, 1] = 0;",
              "    arcArray[i, 2] = 0;",
              "    arcArray[i, 3] = 0;",
              "    for (j = 0; j < 11; j = j + 1) {",
              "        arrTC[j] = 0;",
              "        research[j] = 0;",
              "        spwnDate[i, j] = 0;",
              "    }",
              "}",
              "QState[0] = 3;"          
          ]        
        },
        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  -120,
          "Pos.y":  -140,
          "Parent":  -1,
          "#":  23,
          "Total.Lines":  79,
          "Code.Type":  "Turn",
          "Code":          
          [
              "for (i = 1; i < 4; i = i + 1) {",
              "    dwd0 = ConStar(GalaxySectors(arr0[i]), 0);",
              "    // Установка стороны главной системы сектора",
              "    StarOwner(dwd0, 1);",
              "    StarSeries(dwd0, 1);",
              "    StarCustomFaction(dwd0, 'AMod_Invaders');",
              "",
              "    // Удаление всех кораблей в главной системе сектора",
              "    for (j = 0; j < StarShips(dwd0); j = j + 1) {",
              "        dwd1 = StarShips(dwd0, j);",
              "        if (ShipInScript(dwd1)) continue;",
              "        if (dwd1 == Player()) continue;",
              "        if (dwd1 == Blazer()) continue;",
              "        if (dwd1 == Keller()) continue;",
              "        if (dwd1 == Terron()) continue;",
              "        ShipDestroy(dwd1, 1);",
              "    }",
              "",
              "    // Всех военных этой системы переносим в эту систему (зачем? чтобы они умерли?)",
              "    for (j = 0; j < StarPlanets(dwd0); j = j + 1) {",
              "        dwd2 = StarPlanets(dwd0, j);",
              "        if (PlanetOwner(dwd2) != None) PlanetOwner(dwd2, Kling);",
              "        for (k = 0; k < PlanetWarriors(dwd2); k = k + 1) TransferShip(PlanetWarriors(dwd2, k), dwd0);",
              "    }",
              "",
              "    // Создание Неизвестных Объектов",
              "    dwd1 = BuildCustomRuins(dwd0, 'AMod_InvadersRuin', 9);",
              "    ShipType(dwd1, 'AMod_InvadersRuin');",
              "    ShipOwner(dwd2, Maloc);",
              "    ShipJoin(Arks, dwd1);",
              "    ItemLevel(ShipEqInSlot(dwd1, t_Hull), 6);",
              "    ShipSpecialBonuses(dwd1, bonHull, 94);",
              "    ShipSpecialBonuses(dwd1, bonDroid, -1000);",
              "    ShipSpecialBonuses(dwd1, bonWRadius, -2000);",
              "    ShipSpecialBonuses(dwd1, bonDef, -200);",
              "    ShipSpecialBonuses(dwd1, bonHook, -1000);",
              "    ShipCustomFaction(dwd1, 'AMod_Invaders');",
              "    ShipFace(dwd1, 0);",
              "    Chameleon(dwd1, 'Ruins.InvadersArk');",
              "    ItemSize(ShipEqInSlot(dwd1, t_Hull), 50000);",
              "    ShipRepairEq(dwd1);",
              "    for (j = 1; j < ShipItems(dwd1); j = j + 1) {",
              "        NoDropItem(ShipItems(dwd1, j), 1);",
              "    }",
              "    if (i == 1) {",
              "        SetName(dwd1, 'Альфа');",
              "        dwd2 = ShipEqInSlot(dwd1, t_Hull);",
              "        LinkItemToScript(dwd2, qItem1);",
              "        ItemOnActCode(dwd2, 'if(ScriptItemActionType()==t_OnTakingDamage)ScriptItemActParam(0);if(ScriptItemActionType()==t_OnGettingWeaponHit){if(ScriptItemActObject1()==Player()){if(ItemType(ScriptItemActObject2())==t_CustomWeapon){if(CustomWeaponType(ScriptItemActObject2())==gunName[0])gunName0=gunName[0];}}}if(ScriptItemActionType()==t_OnTakingDamageEn){if(gunName0==gunName[0]){int0=ScriptItemActParam()*25;ScriptItemActParam(int0);arcArray[1,1]=arcArray[1,1]+int0;gunName0=0;}else ScriptItemActParam(0);}if(ScriptItemActionType()==t_OnTakingDamageSp){if(gunName0==gunName[0]){int0=ScriptItemActParam()*25;ScriptItemActParam(int0);arcArray[1,1]=arcArray[1,1]+int0;gunName0=0;}else ScriptItemActParam(0);}if(ScriptItemActionType()==t_OnTakingDamageMi){if(CustomMissileType(ScriptItemActObject1())==gunName[0]){int0=ScriptItemActParam()*13;ScriptItemActParam(int0);arcArray[1,1]=arcArray[1,1]+int0;}else ScriptItemActParam(0);}');",
              "        dwd2 = CreateQuestItem('ArksItem', 6);",
              "        EqCustomFaction(dwd2, 'AMod_Invaders');",
              "        LinkItemToScript(dwd2, DqItem1);",
              "        AddItemToShip(dwd1, dwd2);",
              "    }",
              "    if (i == 2) {",
              "        SetName(dwd1, 'Бетта');",
              "        dwd2 = ShipEqInSlot(dwd1, t_Hull);",
              "        LinkItemToScript(dwd2, qItem2);",
              "        ItemOnActCode(dwd2, 'if(ScriptItemActionType()==t_OnTakingDamage)ScriptItemActParam(0);if(ScriptItemActionType()==t_OnGettingWeaponHit){if(ScriptItemActObject1()==Player()){if(ItemType(ScriptItemActObject2())==t_CustomWeapon){if(CustomWeaponType(ScriptItemActObject2())==gunName[0])gunName0=gunName[0];}}}if(ScriptItemActionType()==t_OnTakingDamageEn){if(gunName0==gunName[0]){int0=ScriptItemActParam()*25;ScriptItemActParam(int0);arcArray[2,1]=arcArray[2,1]+int0;gunName0=0;}else ScriptItemActParam(0);}if(ScriptItemActionType()==t_OnTakingDamageSp){if(gunName0==gunName[0]){int0=ScriptItemActParam()*25;ScriptItemActParam(int0);arcArray[2,1]=arcArray[2,1]+int0;gunName0=0;}else ScriptItemActParam(0);}if(ScriptItemActionType()==t_OnTakingDamageMi){if(CustomMissileType(ScriptItemActObject1())==gunName[0]){int0=ScriptItemActParam()*13;ScriptItemActParam(int0);arcArray[2,1]=arcArray[2,1]+int0;}else ScriptItemActParam(0);}');",
              "        dwd2 = CreateQuestItem('ArksItem', 6);",
              "        EqCustomFaction(dwd2, 'AMod_Invaders');",
              "        LinkItemToScript(dwd2, DqItem2);",
              "        AddItemToShip(dwd1, dwd2);",
              "    }",
              "    if (i == 3) {",
              "        SetName(dwd1, 'Гамма');",
              "        dwd2 = ShipEqInSlot(dwd1, t_Hull);",
              "        LinkItemToScript(dwd2, qItem3);",
              "        ItemOnActCode(dwd2, 'if(ScriptItemActionType()==t_OnTakingDamage)ScriptItemActParam(0);if(ScriptItemActionType()==t_OnGettingWeaponHit){if(ScriptItemActObject1()==Player()){if(ItemType(ScriptItemActObject2())==t_CustomWeapon){if(CustomWeaponType(ScriptItemActObject2())==gunName[0])gunName0=gunName[0];}}}if(ScriptItemActionType()==t_OnTakingDamageEn){if(gunName0==gunName[0]){int0=ScriptItemActParam()*25;ScriptItemActParam(int0);arcArray[3,1]=arcArray[3,1]+int0;gunName0=0;}else ScriptItemActParam(0);}if(ScriptItemActionType()==t_OnTakingDamageSp){if(gunName0==gunName[0]){int0=ScriptItemActParam()*25;ScriptItemActParam(int0);arcArray[3,1]=arcArray[3,1]+int0;gunName0=0;}else ScriptItemActParam(0);}if(ScriptItemActionType()==t_OnTakingDamageMi){if(CustomMissileType(ScriptItemActObject1())==gunName[0]){int0=ScriptItemActParam()*13;ScriptItemActParam(int0);arcArray[3,1]=arcArray[3,1]+int0;}else ScriptItemActParam(0);}');",
              "        dwd2 = CreateQuestItem('ArksItem', 6);",
              "        EqCustomFaction(dwd2, 'AMod_Invaders');",
