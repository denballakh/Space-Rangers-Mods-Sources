          "Code.Type":  "Init",
          "Code":          
          [
              "//код был занесен в function для использования в циклах",
              "function BonCalc() {",
              "    eqbon=EqSpecial(shipeq);",
              "    if (eqbon!=-1 && eqbon!=0) //если оборудование отсутствует в слоте, то EqSpecial возвращает 0, если есть и на нём нету акрина, то -1",
              "    {",
              "        bonvalue=BonusValue(eqbon,itbon);",
              "        cntbon=cntbon+bonvalue;",
              "        if (bonvalue!=0)",
              "        {",
              "            j=j+1;",
              "            if (bonvalue>0)",
              "            {",
              "                if (bonvalue>0 && j==1) {",
              "                    bonhist=\"<color=0,255,0>\"+bonvalue+\"</color>\";",
              "                    eqhist=\"<color=0,255,0>\"+iteq+\"</color>\";",
              "                }",
              "                else {",
              "                    bonhist=bonhist+\"<color=0,255,0>+\"+bonvalue+\"</color>\";",
              "                    eqhist=eqhist+\"<color=0,255,0>+\"+iteq+\"</color>\";",
              "                }",
              "            }",
              "            else if (bonvalue<0) {",
              "                bonhist=bonhist+\"<color=255,0,0>\"+bonvalue+\"</color>\";",
              "                eqhist=eqhist+\"<color=255,0,0>-\"+iteq+\"</color>\";",
              "            }",
              "        }",
              "    }",
              "}"          
          ]        
        },
        {
