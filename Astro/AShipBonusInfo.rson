          "Code.Type":  "Turn",
          "Code":          
          [
              "DAnswer('restart~'+\"<color=136,0,0>Выход</color>\");",
              "InjectAnswer('aCalc',\"Подсчитать бонусы игрока\",Player());",
              "InjectAnswer('aShipBonusInfo',\"Подсчитать бонусы корабля/базы\",0);",
              "InjectAnswer('aBonusInfo',\"<color=0,0,255>Опции:</color>\",0);",
              "",
              "if (!opspecbon) InjectAnswer('aOpOnOff',\"Показывать спец. бонус\",1);",
              "else InjectAnswer('aOpOnOff',\"Не показывать спец. бонус\",2);",
              "",
              "if (!opbonhist) InjectAnswer('aOpOnOff',\"Показывать историю бонусов\",3);",
              "else InjectAnswer('aOpOnOff',\"Не показывать историю бонусов\",4);",
              "",
              "if (!opeqhist) InjectAnswer('aOpOnOff',\"Показывать историю оборудований\",5);",
              "else InjectAnswer('aOpOnOff',\"Не показывать историю оборудований\",6);"          
          ]        
        },
        {
          "Type":  "Top",
          "Name":  "",
          "Pos.x":  880,
          "Pos.y":  650,
          "Parent":  -1,
          "#":  38,
          "Total.Lines":  144,
          "Code.Type":  "Turn",
          "Code":          
          [
              "text=\"<clr>---Текущая сумма и разница бонусов у корабля/базы---<clrEnd><br>\";",
              "text=text+\"<clr>--<Name(target)> (система <Name(ShipStar(target))>)--<clrEnd><br>\";",
              "text=text+\"<color=90,200,220>-Оборудование-</color><br>\";",
              "for (i=1;i<=32;i=i+1)",
              "{",
              "    cntbon=0; //сброс переменной со значением бонуса оборудования, чтобы значение предыдущего бонуса не суммировалось с бонусом последующего",
              "    bonhist=\"\";",
              "    eqhist=\"\";",
              "    history=\"\";",
              "    //переменная i нужна для порядка показа и считывания конкретных бонусов",
              "    //Оборудование",
              "    if (i==1) itbon=bonHull;",
              "    //if (i==2) itbon=bonFuel;",
              "    if (i==2) itbon=bonSpeed;",
              "    if (i==3) itbon=bonJump;",
              "    if (i==4) itbon=bonRadar;",
              "    if (i==5) itbon=bonScan;",
              "    if (i==6) itbon=bonDroid;",
              "    if (i==7) itbon=bonHook;",
              "    if (i==8) itbon=bonHookRadius;",
              "    if (i==9) itbon=bonHookMaxSpeed;",
              "    if (i==10) itbon=bonHookMinSpeed;",
              "    if (i==11) itbon=bonDef;",
              "    if (i==12) itbon=bonMass;",
              "    //Оружие",
              "    if (i==13) itbon=bonWEnergy;",
              "    if (i==14) itbon=bonWSplinter;",
              "    if (i==15) itbon=bonWMissile;",
              "    if (i==16) itbon=bonWRadius;",
              "    //Навыки",
              "    if (i==17) itbon=bonSkill1;",
              "    if (i==18) itbon=bonSkill2;",
              "    if (i==19) itbon=bonSkill3;",
              "    if (i==20) itbon=bonSkill4;",
              "    if (i==21) itbon=bonSkill5;",
              "    if (i==22) itbon=bonSkill6;",
              "    //Слоты",
              "    if (i==23) itbon=bonSlotRadar;",
              "    if (i==24) itbon=bonSlotScaner;",
              "    if (i==25) itbon=bonSlotDroid;",
              "    if (i==26) itbon=bonSlotHook;",
              "    if (i==27) itbon=bonSlotDef;",
              "    if (i==28) itbon=bonSlotWeapon;",
              "    if (i==29) itbon=bonSlotArt;",
              "    if (i==30) itbon=bonSlotForsage;",
              "    //Разное",
              "    if (i==31) itbon=bonStimCapacity;",
              "    if (i==32) itbon=bonZonds;",
              "",
              "    if (opspecbon) {",
              "        specbon=ShipSpecialBonuses(target,itbon); //значение этого спец. бонуса (если больше 0, то положительное, если меньше, то негативное)",
              "        if (specbon!=0) {",
              "            if (specbon>0) specbon=\", спец. бонус <color=0,255,0>\"+specbon+\"</color>\"; //сумма спец. бонуса больше 0, т.е. положительная",
              "            else if (specbon<0) specbon=\", спец. бонус <color=255,0,0>\"+specbon+\"</color>\"; //сумма спец. бонуса меньше 0, т.е. отрицательная",
              "        }",
              "        else specbon='';",
              "    }",
              "",
              "    for (j=t_Hull;j<=t_DefGenerator;j=j+1) //перебор оборудования",
              "    {",
              "        shipeq=ShipEqInSlot(target,j);",
              "        if (j==t_Hull) iteq=\"К\";",
              "        if (j==t_FuelTanks) iteq=\"Т\";",
              "        if (j==t_Engine) iteq=\"Дв\";",
              "        if (j==t_Radar) iteq=\"Р\";",
              "        if (j==t_Scaner) iteq=\"С\";",
              "        if (j==t_RepairRobot) iteq=\"Др\";",
              "        if (j==t_CargoHook) iteq=\"З\";",
              "        if (j==t_DefGenerator) iteq=\"Г\";",
              "        BonCalc();",
              "    }",
              "    for (j=1;j<=5;j=j+1) //перебор оружий",
              "    {",
              "        shipeq=ShipEqInSlot(target,t_Weapon1,j);",
              "        if (j==1) iteq=\"О1\";",
              "        if (j==2) iteq=\"О2\";",
              "        if (j==3) iteq=\"О3\";",
              "        if (j==4) iteq=\"О4\";",
              "        if (j==5) iteq=\"О5\";",
              "        BonCalc();",
              "    }",
              "",
              "        if (cntbon==0); eqbon=\"0\";",
              "        if (cntbon>0) eqbon=\"<color=0,255,0>\"+cntbon+\"</color>\";",
              "        if (cntbon<0) eqbon=\"<color=255,0,0>\"+cntbon+\"</color>\";",
              "",
              "        if (opbonhist && opeqhist) history=\" (\"+bonhist+\"||\"+eqhist+\")\";",
              "        else if (!opbonhist && opeqhist) history=\" (\"+eqhist+\")\";",
              "        else if (opbonhist && !opeqhist) history=\" (\"+bonhist+\")\";",
              "",
              "        if (history==\" ()\") history=\"\";",
              "        else if (history==\" (||)\") history=\"\";",
              "",
              "    if (i==1) text=text+\"К броне корпуса: <eqbon><history><specbon><br>\";",
              "    // if (i==2) text=text+\"К объему топл. бака: <eqbon><history><specbon><br>\";",
              "    if (i==2) text=text+\"К скор. двигателя: <eqbon><history><specbon><br>\";",
              "    if (i==3) text=text+\"К дальн. прыжка двиг.: <eqbon><history><specbon><br>\";",
              "    if (i==4) text=text+\"К дальн. радара: <eqbon><history><specbon><br>\";",
              "    if (i==5) text=text+\"К мощ. сканера: <eqbon><history><specbon><br>\";",
              "    if (i==6) text=text+\"К эфф. починки дроида: <eqbon><history><specbon><br>\";",
              "    if (i==7) text=text+\"К мощ. захвата: <eqbon><history><specbon><br>\";",
              "    if (i==8) text=text+\"К дальн. действия захвата: <eqbon><history><specbon><br>\";",
              "    if (i==9) text=text+\"К скор. притяж. захвата на дальн. дист.: <eqbon><history><specbon><br>\";",
              "    if (i==10) text=text+\"К скор. притяж. захвата на ближ. дист.: <eqbon><history><specbon><br>\";",
              "    if (i==11) text=text+\"К мощ. ГЗП: <eqbon><history><specbon><br>\";",
              "    if (i==12) text=text+\"К % общей массы корабля: <eqbon><history><specbon><br>\";",
              "    if (i==13) {",
              "        text=text+\" <br><color=255,120,60>-Оружие-</color><br>\";",
              "        text=text+\"К силе энергетического оружия: <eqbon><history><specbon><br>\";",
              "    }",
              "    if (i==14) text=text+\"К силе осколочного оружия: <eqbon><history><specbon><br>\";",
              "    if (i==15) text=text+\"К силе ракетного оружия: <eqbon><history><specbon><br>\";",
              "    if (i==16) text=text+\"К дальности стрельбы оружия: <eqbon><history><specbon><br>\";",
              "    if (i==17) {",
              "        text=text+\" <br><color=0,150,0>-Навыки-</color><br>\";",
              "        text=text+\"Навык точности: <eqbon><history><specbon><br>\";",
              "    }",
              "    if (i==18) text=text+\"Навык манёвренности: <eqbon><history><specbon><br>\";",
              "    if (i==19) text=text+\"Навык знания техники: <eqbon><history><specbon><br>\";",
              "    if (i==20) text=text+\"Навык торговли: <eqbon><history><specbon><br>\";",
              "    if (i==21) text=text+\"Навык обаяния: <eqbon><history><specbon><br>\";",
              "    if (i==22) text=text+\"Навык лидерства: <eqbon><history><specbon><br>\";",
              "    if (i==23) {",
              "    text=text+\" <br><color=100,90,0>-Слоты-</color><br>\";",
              "    text=text+\"Слот радара: <eqbon><history><specbon><br>\";",
              "    }",
              "    if (i==24) text=text+\"Слот сканера: <eqbon><history><specbon><br>\";",
              "    if (i==25) text=text+\"Слот дроида: <eqbon><history><specbon><br>\";",
              "    if (i==26) text=text+\"Слот захвата: <eqbon><history><specbon><br>\";",
              "    if (i==27) text=text+\"Слот ГЗП: <eqbon><history><specbon><br>\";",
              "    if (i==28) text=text+\"Слоты оружия: <eqbon><history><specbon><br>\";",
              "    if (i==29) text=text+\"Слоты артефактов: <eqbon><history><specbon><br>\";",
              "    if (i==30) text=text+\"Слот форсажа: <eqbon><history><specbon><br>\";",
              "    if (i==31) {",
              "        text=text+\" <br><color=160,70,160>-Разное-</color><br>\";",
              "        text=text+\"К макс. кол-ву принимаемых за раз стимуляторов: <eqbon><history><specbon><br>\";",
              "    }",
              "    if (i==32) text=text+\"К макс. лимиту зондов под контролем игрока: <eqbon><history><specbon><br>\";",
              "}",
              "",
              "Sound('Sound.NewMsg');",
              "//Т.к. Ether с указателем на корабль не хочет появляться, если корабль не находится в системе с игроком или игрок вызвал эзер, находясь в космосе, а корабль на базе/планете, то сделаем проверку:",
              "if (ShipStar(target)==ShipStar(Player()) && ShipInNormalSpace(Player()) && ShipInNormalSpace(target)) CustomEther('Bonus',7,'',text,target); //если корабль находится в системе с игроком и игрок с кораблём находятся в обычном космосе, то выводится Ether с указателем на этот корабль",
              "else CustomEther('Bonus',7,'',text); //в противном случае без указателя"          
          ]        
